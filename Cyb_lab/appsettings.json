{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=cyb_lab.db;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordPolicy": {
    "RequiredLength": 14,
    "RequiredUniqueChars": 0,
    "RequireNonAlphanumeric": false,
    "RequireLowercase": false,
    "RequireUppercase": false,
    "RequireDigit": true,
    "ExpirationTime": "30.00:00:00"
  }
}